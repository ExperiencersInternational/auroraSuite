-- Cloning of build, codename and version to UI before running anything else for sanity reasons

local p = script.Parent
local auroraUI = p.UI

local buildc = p.Build:Clone()
local codenamec = p.Codename:Clone()
local versionc = p.Version:Clone()

buildc.Parent = auroraUI
codenamec.Parent = auroraUI
versionc.Parent = auroraUI

-- All the regular stuff

local toolbar = plugin:CreateToolbar("auroraSuite " .. script.Parent.Version.Value) -- Creates a toolbar for the plugin
local toolbarBTN = toolbar:CreateButton("Aurora Menu", "Opens the auroraSuite menu.", "rbxassetid://12757813093") -- Creates a button inside the toolbar (Parameters: Name, Description, Image ID for button)

local Plugin = plugin:CreateDockWidgetPluginGui(
	"auroraSuite",
	DockWidgetPluginGuiInfo.new(
		Enum.InitialDockState.Float,
		false,
		false,
		280, -- Default Width
		540, -- Default Height
		260, -- Minimum width
		480 -- Minimum height
	)
)

Plugin.Title = "auroraSuite"
Plugin.Name = "experiencersinternational.aurorasuite.main"

local MaxSize = 12
local MinSize = 6
local ThicknessSize = 0
local IsBorder = false
local AutoAdjustZIndex = true
local GlobalZindex = true

toolbarBTN.Click:Connect(function()
	Plugin.Enabled = not Plugin.Enabled
end)

local BaseGui = script.Parent.UI
BaseGui.Parent = Plugin

local uptime = plugin:GetSetting("PluginUptime")

if uptime == nil then
	plugin:SetSetting("PluginUptime", 0)
end

local debugging = plugin:GetSetting("DebugMode")

if debugging == nil then
	plugin:SetSetting("DebugMode", false)
end

while true do
	task.wait(60)
	local uptime2 = plugin:GetSetting("PluginUptime") -- seems to not be counting correctly so I had to add this
	plugin:SetSetting("PluginUptime", uptime2 + 1)
end]]></ProtectedString>
