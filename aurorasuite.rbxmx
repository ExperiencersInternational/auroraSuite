								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local selection = game:GetService("Selection")
	local history = game:GetService("ChangeHistoryService")
	local selected = selection:Get()

	if #selected == 0 then
		warn("auroraSuite: Nothing is selected to iterate on.")
		return
	end
	
	history:SetWaypoint("auroraSuite: before")
	
	-- Iterate test
	
	for i, v in selected do
		local testfolder = Instance.new("Folder", v.Parent)
		testfolder.Name = "auroratest"
		local success = pcall(function()
			v.Parent = v.Parent.auroratest
		end)
		if success then
			v.Parent = v.Parent.Parent
			testfolder:Destroy()
		else
			v:SetAttribute("auroraSuite_IterateIgnore", true)
			if plugin:GetSetting("Debug") == true then
				print("auroraSuite: Item can not be iterated " .. v:GetFullName())
			end
			testfolder:Destroy()
		end
	end
	
	-- Iteration
	
	local iteratevalue = 1
	for x, y in selected do
		if y:GetAttribute("auroraSuite_IterateIgnore") == true then
			
		else
			y.Name = "Instance" .. tostring(iteratevalue)
			iteratevalue = iteratevalue + 1
		end
	end
	
	history:SetWaypoint("auroraSuite: after")
	print("auroraSuite: Iterated selected instances.")
end)]]></ProtectedString>
