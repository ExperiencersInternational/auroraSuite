								<ProtectedString name="Source"><![CDATA[local setting = plugin:GetSetting("ViewportDisplay")
local RunService = game:GetService("RunService")

if game.CoreGui:FindFirstChild("auroraSuite") then
	if plugin:GetSetting("DebugMode") == true then
		print("auroraSuite: Initializing viewport display tool was not needed.")
	end
else
	local SUI = Instance.new("ScreenGui", game.CoreGui)
	SUI.Archivable = false
	SUI.Name = "auroraSuite"
	SUI:SetAttribute("GroupMe_Ignore", true)
	SUI.IgnoreGuiInset = true
	local frame = Instance.new("Frame", SUI)
	frame.Archivable = false
	frame.Size = UDim2.new(1,0,1,0)
	frame.BackgroundTransparency = 1
	frame.Visible = false
	local textlabel = Instance.new("TextLabel", frame)
	textlabel.Archivable = false
	textlabel.Text = "initialising"
	textlabel.Font = Enum.Font.Gotham
	textlabel.BackgroundTransparency = 0.5
	textlabel.BorderSizePixel = 0
	if RunService:IsRunning() then
		if plugin:GetSetting("ViewportResolutionRight") == true then
			textlabel.AnchorPoint = Vector2.new(1,0)
			textlabel.Position = UDim2.new(1,-52,0,0)
		else
			textlabel.Position = UDim2.new(0,96,0,0)
		end
	else
		if plugin:GetSetting("ViewportResolutionRight") == true then
			textlabel.AnchorPoint = Vector2.new(1,0)
			textlabel.Position = UDim2.new(1,0,0,0)
		end
	end
	textlabel.Size = UDim2.new(0, 80, 0, 24)
	textlabel.TextSize = "20"
	textlabel.TextXAlignment = Enum.TextXAlignment.Left
	textlabel.AutomaticSize = Enum.AutomaticSize.X
	local padding = Instance.new("UIPadding", textlabel)
	padding.Archivable = false
	padding.PaddingLeft = UDim.new(0, 8)
	padding.PaddingRight = UDim.new(0, 8)
end

if setting == nil then
	plugin:SetSetting("ViewportDisplay", false)
	
	print("auroraSuite: Setting for viewport resolution display was not found, automatically set to default (false). If this is your first time using auroraSuite, or if you have just updated to version 1.1.0 or higher, ignore this message.")
end

if setting == true then
	script.Parent.Text = "Disable"
	script.Parent.Parent.StatusLabel.Text = "Currently enabled"
	game.CoreGui.auroraSuite.Frame.Visible = true
	
	if plugin:GetSetting("DebugMode") == true then
		print("auroraSuite: Enabled visibility of viewport resolution display.")
	end
end

if setting == false then
	script.Parent.Text = "Enable"
	script.Parent.Parent.StatusLabel.Text = "Currently disabled"
	game.CoreGui.auroraSuite.Frame.Visible = false
	
	if plugin:GetSetting("DebugMode") == true then
		print("auroraSuite: Disabled visibility of viewport resolution display.")
	end
end

local function refresh()
	game.CoreGui.auroraSuite.Frame.TextLabel.Text = game.CoreGui.auroraSuite.Frame.AbsoluteSize.X .. " x " .. game.CoreGui.auroraSuite.Frame.AbsoluteSize.Y
end

refresh()

game.CoreGui.auroraSuite.Frame:GetPropertyChangedSignal("AbsoluteSize"):Connect(refresh)

script.Parent.MouseButton1Click:Connect(function()
	local settingsanitycheck = plugin:GetSetting("ViewportDisplay")
	
	if settingsanitycheck == true then
		script.Parent.Text = "Enable"
		script.Parent.Parent.StatusLabel.Text = "Currently disabled"
		plugin:SetSetting("ViewportDisplay", false)
		game.CoreGui.auroraSuite.Frame.Visible = false
		print("auroraSuite: Disabled visibility of viewport resolution display.")
	else
		script.Parent.Text = "Disable"
		script.Parent.Parent.StatusLabel.Text = "Currently enabled"
		plugin:SetSetting("ViewportDisplay", true)
		game.CoreGui.auroraSuite.Frame.Visible = true
		print("auroraSuite: Enabled visibility of viewport resolution display.")
	end
end)]]></ProtectedString>
