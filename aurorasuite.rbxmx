								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local box = script.Parent.Parent.WalkspeedBox
	local ws = box.Text
	local history = game:GetService("ChangeHistoryService")
	local scs = game.StarterPlayer.StarterCharacterScripts
	local action = script.Parent.ShiftToSprint
	local record = history:TryBeginRecording("auroraSuite: Shift to Sprint")
	
	if record then
		ws = tonumber(ws)

		if ws == nil or ws == "" then
			ws = 30
			warn("auroraSuite was unable to detect a walkspeed value inputted into the Shift to Sprint tool. It has been automatically set to 30.")
		end

		if scs:FindFirstChild("Action") then
			scs.Action:SetAttribute("Walkspeed", ws)
			warn("auroraSuite already found an existing shift to sprint script, so we've just changed the values to what you've set.")
		else
			local clone = action:Clone()
			clone.Parent = scs
			clone.Disabled = false
			clone:SetAttribute("Walkspeed", ws)
		end
	end
	
	history:FinishRecording(record, Enum.FinishRecordingOperation.Commit)
	
	if plugin:GetSetting("DebugMode") == true then
		print("auroraSuite has successfully created (or made changes to) a shift to sprint script.")
	end
end)]]></ProtectedString>
