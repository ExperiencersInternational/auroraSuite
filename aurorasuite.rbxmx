									<ProtectedString name="Source"><![CDATA[-- Majority ported from blisstools which received assistance from @csqrl. Consider buying some of his plugins to support him, he makes some really good ones such as Codify!

local ChangeHistoryService = game:GetService("ChangeHistoryService")
local MarketplaceService = game:GetService("MarketplaceService")
local Selection = game:GetService("Selection")
local CollectionService = game:GetService("CollectionService")

local tta = "13683500513"

local assetCache = {}
local imageIdCache = {}

local MESSAGES = {
	EMPTY_STRING = "auroraSuite: An image or decal ID could not be found in the texture applier textbox, make sure that you've inserted an ID in.",
	NO_SELECTION = "auroraSuite: No items was selected to apply textures to, have you checked that you've applied textures?",
	SUCCESS = "auroraSuite: Applied textures to parts successfully.",
}

local function GetAssetDetails(assetId: number)
	if assetCache[assetId] then
		return assetCache[assetId]
	end

	local details = MarketplaceService:GetProductInfo(assetId, Enum.InfoType.Asset)
	assetCache[assetId] = details

	return details
end

local function GetImageAssetId(assetId: number | string): number
	local assetId = if type(assetId) == "string"
		then tonumber(assetId:match("%d+"))
		else assetId

	if type(assetId) ~= "number" then
		error("Unable to parse Asset ID", 2)
	end

	if imageIdCache[assetId] then
		return imageIdCache[assetId]
	end

	local details = GetAssetDetails(assetId)

	if details.AssetTypeId == Enum.AssetType.Decal.Value then
		local decal = game:GetObjects("rbxassetid://" .. assetId)[1]

		if decal then
			local textureId = decal.Texture:match("%d+$")

			decal:Destroy()
			imageIdCache[assetId] = textureId

			return textureId
		end
	elseif details.AssetTypeId == Enum.AssetType.Image.Value then
		imageIdCache[assetId] = assetId
		return assetId
	end

	error("Asset ID does not point to an Image or Decal", 2)
end

local function ApplyTexture(assetId: number, target: Instance, face: Enum.NormalId)
	local texture = Instance.new("Texture")

	texture.Face = face
	texture.Texture = "rbxassetid://" .. assetId
	texture.Name = face.Name .. "Texture"
	texture.Parent = target

	CollectionService:AddTag(texture, "auroraSuite-generated")
end

script.Parent.Activated:Connect(function()
	local ok, imageAssetId = pcall(GetImageAssetId, tta)

	if not ok then
		warn(imageAssetId)
		return
	end

	local record = ChangeHistoryService:TryBeginRecording("auroraSuite: Texture Applier")

	if record then
		local selection = Selection:Get()

		if #selection == 0 then
			warn(MESSAGES.NO_SELECTION)
			return
		end

		for _, instance in ipairs(selection) do
			if not instance:IsA("BasePart") then
				continue
			end

			for _, face in ipairs(Enum.NormalId:GetEnumItems()) do
				ApplyTexture(imageAssetId, instance, face)
			end
		end
	end
	ChangeHistoryService:FinishRecording(record, Enum.FinishRecordingOperation.Commit)
	print(MESSAGES.SUCCESS)
end)]]></ProtectedString>
