								<ProtectedString name="Source"><![CDATA[-- Thanks to luisgamercooI231fan for submitting these changes
local TabletJumpbuttonYoffset = 2.55 -- the mobile button offset on the Y axis(up) for tablets
local PhoneJumpButtonoffset = 75 --the mobile button offset on the Y axis(up) for phones
local CurrentCamera = workspace.CurrentCamera
local MinAxis = math.min(CurrentCamera.ViewportSize.X, CurrentCamera.ViewportSize.Y)
local IsSmallScreen = MinAxis <= 500
local ActionButtonSize = IsSmallScreen and 70 or 120
local UIS = game:GetService("UserInputService")
local Icon = "rbxassetid://6034754445" -- button icon for the mobile button
local StargetPlayer = game:GetService("StarterPlayer")
local RunService = game:GetService("RunService")
local CAS = game:GetService("ContextActionService")
local keys = { -- keys that are compatible with this.
	--[Computer]--
	Enum.KeyCode.LeftShift, 
	Enum.KeyCode.RightShift,
	--[Xbox]--
	Enum.KeyCode.ButtonR2 -- R2 button on PS4/PS5 controllers, ZR on Nintendo controllers, and RT on Xbox controllers
}
local Sprinting = false
local function Sprint(Name, State)
	if State == Enum.UserInputState.Begin then
		Sprinting = true
	elseif Enum.UserInputState.End then -- once they let go
		Sprinting = false
	
	end
end
local function RescaleActionButton(Name) -- scale button on mobile
	local ActionButton = CAS:GetButton(Name)

	if ActionButton then
		local ActionTitle = ActionButton:WaitForChild("ActionTitle")

		if not IsSmallScreen then
			ActionTitle.TextSize = 36
		else
			ActionTitle.TextSize = 18
		end

		ActionButton.Size = UDim2.fromOffset(ActionButtonSize, ActionButtonSize)
	end
end

CAS:BindAction("SprintAction", Sprint, true, unpack(keys))
CAS:SetImage("SprintAction", Icon)


