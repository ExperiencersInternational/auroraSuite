local SearchBar = script.Parent.SearchBox -- Put the textbox of your searchbar here instead of nil
SearchBar:GetPropertyChangedSignal("Text"):Connect(function()
	local InputText = string.lower(SearchBar.Text)
	
	for _, Tool in pairs(ScrollingFrame:GetChildren()) do
		if Tool:GetAttribute("Searchable") == false then
			
		else
			if Tool:GetAttribute("ShouldBeVisibleAtLaunch") == true then
				Tool.Visible = string.find(string.lower(Tool.TopElements.TextLabel.Text), InputText) and true or false
			else
				Tool.Visible = false
			end
		end
	end
end)]]></ProtectedString>
