		local Char = game.Players.LocalPlayer.Character
		local Hum = Char:WaitForChild("Humanoid")
		if Hum then
			Hum.WalkSpeed = script:GetAttribute("Walkspeed")
		end
	else -- not sprinting
		local Char = game.Players.LocalPlayer.Character
		local Hum = Char:WaitForChild("Humanoid")
		if Hum then
			Hum.WalkSpeed = StargetPlayer.CharacterWalkSpeed
		end
	end
