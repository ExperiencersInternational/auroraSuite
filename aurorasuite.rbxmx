								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local text = script.Parent.Parent.TextBox.Text
	local feesCheck = script.Parent.Parent.RobloxFeesToggle
	local value
	
	if text == nil or text == "" then
		warn("auroraSuite: No value has been given to the DevEx Calculator text box.")
	end
	
	if feesCheck.Text == "X" then
		value = 0.35
	else
		value = 0.245
	end
	
	local output = tonumber(text) * value -- Gets 24.5% of the text
	local conversion = output / 80 -- 80 Robux is one dollar, or 100 cents
	
	script.Parent.Parent.OutputLabel.Text = tostring(conversion) .. " US$"
	print("auroraSuite: Converted Robux to USD after DevEx and/or Roblox fees, result: " .. tostring(conversion))
end)]]></ProtectedString>
