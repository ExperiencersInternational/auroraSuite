								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local box = script.Parent.Parent.TextBox
	local selection = game:GetService("Selection")
	local history = game:GetService("ChangeHistoryService")
	
	if box.Text == "" or box.text == nil or box.text == "Inserted" then
		warn("auroraSuite: Could not select instances of class name due to selection being empty.")
		return
	end
	
	local selected = selection:Get()
	
	if #selected == 0 then
		warn("auroraSuite: Could not select instances of class name due to selection being empty.")
		return
	end
	
	history:SetWaypoint("auroraSuite: Before")
	
	local childselection = {}
	local iterate = 1
	
	for i, v in ipairs(selected) do
		local array = v:GetChildren()
		for x, y in ipairs(array) do
			if y.ClassName == box.Text then
				childselection[iterate] = y
				iterate = iterate + 1
			end
		end
	end
	
	selection:Set(childselection)
	
	print("auroraSuite: Selected items (if any) with class name of " .. box.Text)
	history:SetWaypoint("auroraSuite: After")
end)]]></ProtectedString>
