								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local selection = game:GetService("Selection")
	local history = game:GetService("ChangeHistoryService")
	local selected = selection:Get()
	local array = {}

	if #selected == 0 then
		warn("auroraSuite: No instances are selected to select children of those instances.")
		return
	end
	
	history:SetWaypoint("auroraSuite: before")
	
	for i, v in selected do
		local list = v:GetChildren()
		
		for x, y in list do
			local check = #array
			array[#array + 1] = y
		end
	end
	
	selection:Set(array)
	
	history:SetWaypoint("auroraSuite: after")
	print("auroraSuite: Selected children of the previously selected instances.")
end)]]></ProtectedString>
