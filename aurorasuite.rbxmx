								<ProtectedString name="Source"><![CDATA[script.Parent.MouseButton1Click:Connect(function()
	local box = script.Parent.Parent.TextBox
	local price = script.Parent.Parent.PriceBox
	
	local History = game:GetService("ChangeHistoryService")
	local Tag = game:GetService("CollectionService")
	
	if game.StarterGui:FindFirstChild("auroraSuite") then
		if game.StarterGui.auroraSuite:FindFirstChild("DonationUI") then
			warn("auroraSuite: Donation button tool failed to function due to finding that it's already set up. Multiple button support is coming soon.")
			return
		end
	end
	
	if box.Text == "" or box.Text == nil or box.text == "Created" or price.Text == "" or price.Text == nil  then
		warn("auroraSuite: Donation button was not created due to empty text in either the developer product box, the price box or both.")
		return
	end
	
	local record = History:TryBeginRecording("auroraSuite: Donation Button Creation")
	
	if record then
		local folder = Instance.new("Folder", game.StarterGui)
		folder.Name = "auroraSuite"
		local ScreenGUI = Instance.new("ScreenGui", folder)
		ScreenGUI.Name = "DonationUI"
		ScreenGUI:SetAttribute("GroupMe_Ignore", true)
		Tag:AddTag(folder, "auroraSuite-generated")
		Tag:AddTag(ScreenGUI, "auroraSuite-generated")

		local donatebutton = Instance.new("TextButton", ScreenGUI)
		donatebutton.Position = UDim2.new(0,8,0.4,0)
		donatebutton.Size = UDim2.new(0.1,0,0.08,0)
		donatebutton.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
		donatebutton.Text = 'Donate î' .. price.Text
		donatebutton:SetAttribute("Price", price.Text)
		donatebutton.Font = 'Gotham'
		donatebutton.TextScaled = true
		donatebutton.BorderSizePixel = 0
		local donatescript = Instance.new("LocalScript")
		donatescript.Source = "-- Created with auroraSuite\nlocal id = ".. box.Text
			.."\nlocal plr = game:GetService('Players')\nscript.Parent.MouseButton1Click:Connect(function() \ngame.MarketplaceService:PromptProductPurchase(plr.LocalPlayer, id) \nend)"
		donatescript.Parent = donatebutton
		local UICorner = Instance.new('UICorner')
		UICorner.Parent = donatebutton
		local Padding = Instance.new("UIPadding")
		Padding.Parent = donatebutton
		Padding.PaddingTop = UDim.new(0,4)
		Padding.PaddingLeft = UDim.new(0,4)
		Padding.PaddingBottom = UDim.new(0,4)
		Padding.PaddingRight = UDim.new(0,4)
	end
	
	History:FinishRecording(record, Enum.FinishRecordingOperation.Commit)
	
	print("auroraSuite: Successfully created a donation button.")
	box.Text = ""
	price.Text = ""
end)]]></ProtectedString>
